.box{
  width: 100%;
  height: 1500rpx;
  margin: 0;
  padding: 0;
  background: #fbfbfb;
}
.logo{
    width: 100%;
    height: 370rpx;
    position: relative;
}
.logo image{
  width: 100%;
  height: 100%;
}
.h-posi{
  width: 90%;
  height: 380rpx;
  position: absolute;
  top: 300rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
  background: white;
  box-shadow: 0rpx 0rpx .03rpx 0rpx gray;
  color: gray;
}
.h-in{
  width: 100%;
  height: 28%;
  display: flex;
  align-items: center;
  text-indent: 20rpx;
  font-size: 45rpx;
  font-weight: bold;
  color: black;
}
.h-in1{
  width: 100%;
  height: 23%;
  display: flex;
  align-items: center;
  justify-content: space-between
}
.h-in1 view:nth-child(1),.h-in1 view:nth-child(3){
  width: 20%;
  height: 100%;
  display: flex;
  align-items: center;
  font-size: 34rpx;
}
.h-in1 view:nth-child(1){
  text-indent: 20rpx;
}
.h-in1 view:nth-child(3) button{
  width: 130rpx;
  height: 70rpx;
background: #f9f7fa;
text-align: center;
font-size: 30rpx;
}
.h-in1 view:nth-child(2){
  width: 60%;
  height: 100%;
  display: flex;
  align-items: center;
  font-size: 35rpx;
}
.h-jiu{
  width: 100%;
  height: 240rpx;
  margin-top: 350rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.h-jiu1{
  width: 33%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.h-jiu1 view:nth-child(1){
  width: 100%;
  height: 60%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.h-jiu1 view:nth-child(1) image{
  width: 56%;
  height: 100%;
}
.h-jiu1 view:nth-child(2){
  width: 100%;
  height: 40%;
  display: flex;
  align-items: center;
  justify-content:center;
}